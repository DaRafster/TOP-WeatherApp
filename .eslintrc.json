{
    "extends": "airbnb-base",
    "rules": {
        "no-unused-vars" : "warn",
        "no-console" : "off",
        "func-names" : "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "comma-dangle" : "off",
        "no-plusplus" : "off"
    },
    "env":{
        "browser": true
    }
}